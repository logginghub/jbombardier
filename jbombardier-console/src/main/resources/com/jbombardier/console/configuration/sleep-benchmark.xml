<interactiveConfiguration transactionRateModifier="1" externalTelemetryHub="localhost" telemetryHubPort="58773" maximumResultToStore="1000">
	
	<agents>
		<agent name="embedded"/>		
	</agents>
	
	<tests>
	    <test name='sleep-10@10-before30' class='com.jbombardier.console.sample.SleepTest' targetRate='10' rateStep='100' properties="delay=10,beforeDelay=30"/>
	    <test name='sleep-25@25' class='com.jbombardier.console.sample.SleepTest' targetRate='25' rateStep='100' properties="delay=25"/>
	    <test name='sleep-5@200' class='com.jbombardier.console.sample.SleepTest' targetRate='200' rateStep='100' properties="delay=5"/>
	    <test name='sleep-10@100' class='com.jbombardier.console.sample.SleepTest' targetRate='100' rateStep='100' properties="delay=10"/>
		<test name='sleep-25@40' class='com.jbombardier.console.sample.SleepTest' targetRate='40' rateStep='100' properties="delay=25"/>
	    <test name='sleep-50@20' class='com.jbombardier.console.sample.SleepTest' targetRate='20' rateStep='100' properties="delay=50"/>
	    <test name='sleep-100@10' class='com.jbombardier.console.sample.SleepTest' targetRate='10' rateStep='100' properties="delay=100"/>
	    <test name='sleep-200@0.5' class='com.jbombardier.console.sample.SleepTest' targetRate='0.5' rateStep='100' properties="delay=200"/>
	       
	</tests>
	
</interactiveConfiguration>